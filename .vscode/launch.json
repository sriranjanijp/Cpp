{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Run & Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
            "args": [],
            "cwd": "${fileDirname}",
            "MIMode": "gdb",
            "miDebuggerPath": "C:/MinGW/bin/gdb.exe",
            "setupCommands": [
                { "text": "-enable-pretty-printing", "ignoreFailures": true }
            ],
            "externalConsole": false,
            "internalConsoleOptions": "neverOpen"
        }
    ]
}
